#   * 
#   * * 
#   * * * 
#   * * * * 
#   * * * * *

class Solution:
    def printTriangle(self, N):
        for i in range(N):
            for j in range(i+1):
                print("*",end=" ")
            print()    
            
if __name__ == '__main__':
    t = int(input())
    for _ in range(t):
        N = int(input())
        ob = Solution()
        ob.printTriangle(N)
